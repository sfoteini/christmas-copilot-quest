    def get_feather_icon_from_name(icon_name: str) -> str:
        """
        Returns the unicode character for a given icon name. If the icon name is not found, an empty string
        is returned.

        :param icon_name: The name of the icon.

        :return: The unicode character for the icon.
        """
        return FEATHER_ICONS_MAPPING.get(icon_name, "")
